# Environment Variables Template
# Copy this file to .env and customize for your project

# App Configuration
VITE_APP_NAME="My React App"
VITE_APP_VERSION="1.0.0"

# Development Configuration
VITE_DEV_PORT=5173
VITE_DEV_HOST="localhost"

# Feature Flags
VITE_ENABLE_DEV_TOOLS=true
VITE_ENABLE_STRICT_MODE=true

# Playwright Configuration (for visual testing)
# PLAYWRIGHT_BROWSER="chromium"
# PLAYWRIGHT_HEADLESS=false

# Docker Development
# NODE_ENV is automatically set by Docker profiles
# PORT is automatically set by Vite
